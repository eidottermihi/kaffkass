<%= render "shared/error_messages", :target => @consumption %>

<%= form_for [@coffee_box, @consumption] do |f| %>

    <%= field_set_tag "Consumption" do %>
        <ol>
          <li><%= f.label :day %>
            <%= f.date_select :day %></li>
          <li><%= f.label :numberOfCups %>
            <%= f.text_field :numberOfCups %></li>
          <li><%= f.submit %></li>
        </ol>

    <% end %>
<% end %>
